  <!-- SPLASH EFFECT-->
  <Sequence>
    <config_type>CONFIG_sprite_effects</config_type>
    <frames>
      <Frame>
        <type>SPRITE</type>
        <name>SPLASH_PLAYER</name>
        <value>player_splash</value>
        <stoppable>false</stoppable>
        <sound>loosing a life from the enemy</sound>
        <delay>0.2</delay>
        <xOffSet>0</xOffSet>
        <yOffSet>0</yOffSet>
      </Frame>
      <Frame>
        <type>COMMAND</type>
        <name>DELETE</name>
        <parameter>NONE</parameter>
        <stoppable>false</stoppable>
      </Frame>
    </frames>
    <name>SPLASH_PLAYER</name>
    <raised>false</raised>
    <collision>Passable</collision>
  </Sequence>

  <!-- SPLASH EFFECT-->
  <Sequence>
    <config_type>CONFIG_sprite_effects</config_type>
    <frames>
      <Frame>
        <type>SPRITE</type>
        <name>SPLASH_ENEMY</name>
        <value>enemy_splash</value>
        <sound>taking a life to the enemy</sound>
        <stoppable>false</stoppable>
        <delay>0.2</delay>
        <xOffSet>0</xOffSet>
        <yOffSet>0</yOffSet>
      </Frame>
      <Frame>
        <type>COMMAND</type>
        <name>DELETE</name>
        <parameter>NONE</parameter>
        <stoppable>false</stoppable>
      </Frame>
    </frames>
    <name>SPLASH_ENEMY</name>
    <raised>false</raised>
    <collision>Passable</collision>
  </Sequence>

</ArrayOfSequence>

